# ---------------------------------------------------------------------------
!if !$d(BCB)
BCB = $(MAKEDIR)\..
!endif

# ---------------------------------------------------------------------------
# IDE SECTION
# ---------------------------------------------------------------------------
# The following section of the project makefile is managed by the BCB IDE.
# It is recommended to use the IDE to change any of the values in this
# section.
# ---------------------------------------------------------------------------

VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = exoutln.exe
OBJFILES = exoutln.obj ExOutLn0.obj
RESFILES = ExOutLn.res
DEFFILE =
RESDEPEN = $(RESFILES) ExOutLn0.dfm
LIBFILES =
IDLGENFILES =
IDLFILES =
LIBRARIES = o404_r41.lib vclx40.lib vcl40.lib
SPARELIBS = vcl40.lib vclx40.lib o404_r41.lib
PACKAGES = vcl40.bpi vcldb40.bpi
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
SYSDEFINES = NO_STRICT
USERDEFINES =
# ---------------------------------------------------------------------------
CFLAG1 = -I..\..\;..\..\HPP3;$(BCB)\include;$(BCB)\include\vcl -Od -w -Ve -Tkh30000 -r- \
  -a8 -k -y -v -vi- -D$(SYSDEFINES);$(USERDEFINES) -c -b- -w-par -w-inl -Vx -tW \
  -tWM
CFLAG2 =
CFLAG3 =
IDLCFLAGS = -src_suffixcpp -I"..\..\" -I..\..\HPP3 -I$(BCB)\include -I$(BCB)\include\vcl
PFLAGS = -U..\..;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I..\..\;..\..\HPP3;$(BCB)\include;$(BCB)\include\vcl -$YD -$W -$O- -v -M -JPHNE
RFLAGS = -i..\..\;..\..\HPP3;$(BCB)\include;$(BCB)\include\vcl
AFLAGS = /i..\..\ /i..\..\HPP3 /i$(BCB)\include /i$(BCB)\include\vcl /mx /w2 /zi \
  /d_RTLDLL /dUSEPACKAGES
LFLAGS = -L..\..;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) -aa -Tpe -x -Gn
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0w32.obj sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mt.lib
# ---------------------------------------------------------------------------
!ifdef IDEOPTIONS

[Version Info]
IncludeVerInfo=0
AutoIncBuild=0
MajorVer=1
MinorVer=0
Release=0
Build=0
Debug=0
PreRelease=0
Special=0
Private=0
DLL=0
