!if !$d(BCB)
BCB = $(MAKEDIR)\..
!endif

# ---------------------------------------------------------------------------
# IDE SECTION
# ---------------------------------------------------------------------------
# The following section of the project makefile is managed by the BCB IDE.
# It is recommended to use the IDE to change any of the values in this
# section.
# ---------------------------------------------------------------------------

VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = O406_r41.BPL
OBJFILES = AllOr.obj O406_r41.obj
RESFILES = O406_r41.res
DEFFILE =
RESDEPEN = $(RESFILES)
LIBFILES =
IDLGENFILES =
IDLFILES =
LIBRARIES = dclocx40.lib inet40.lib inetdb40.lib dss40.lib tee40.lib teedb40.lib \
  teeui40.lib qrpt40.lib ibsmp40.lib bcbsmp40.lib nmfast40.lib vcldbx40.lib \
  vcldb40.lib vclmid40.lib vcljpg40.lib vclx40.lib
SPARELIBS = Vcl40.lib vclx40.lib vcljpg40.lib vclmid40.lib vcldb40.lib vcldbx40.lib \
  nmfast40.lib bcbsmp40.lib ibsmp40.lib qrpt40.lib teeui40.lib teedb40.lib \
  tee40.lib dss40.lib inetdb40.lib inet40.lib dclocx40.lib
PACKAGES = vcl40.bpi
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
SYSDEFINES = NO_STRICT;USEPACKAGES
USERDEFINES =
# ---------------------------------------------------------------------------
CFLAG1 = -I$(BCB)\include;$(BCB)\include\vcl;$(BCB)\lib;$(BCB)\lib\release -O2 -Hc \
  -H=$(BCB)\lib\vcl40.csm -w -Ve -Tkh30000 -a8 -k- -vi \
  -D$(SYSDEFINES);$(USERDEFINES) -c -b- -w-par -w-inl -Vx -tWM
CFLAG2 =
CFLAG3 =
IDLCFLAGS = -I$(BCB)\include -I$(BCB)\include\vcl -I$(BCB)\lib -I$(BCB)\lib\release \
  -src_suffixcpp
PFLAGS = -U$(BCB)\Lib\release;$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I$(BCB)\include;$(BCB)\include\vcl;$(BCB)\lib;$(BCB)\lib\release -$Y- -$L- \
  -$D- -v -M -JPHNE
RFLAGS = -i$(BCB)\include;$(BCB)\include\vcl;$(BCB)\lib;$(BCB)\lib\release
AFLAGS = /i$(BCB)\include /i$(BCB)\include\vcl /i$(BCB)\lib /i$(BCB)\lib\release /mx /w2 \
  /zn /d_RTLDLL /dUSEPACKAGES
LFLAGS = -L$(BCB)\Lib\release;$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -D"TurboPower Orpheus 4.06 runtime package - VCL41" " -aa -Tpp -Gpr -x -Gn -Gl -Gi
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0pkg32.obj $(PACKAGES) sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mt.lib
# ---------------------------------------------------------------------------
