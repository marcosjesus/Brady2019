  ConfigURL.ProRecepcaoLoteRPS    := '';
  ConfigURL.ProConsultaLoteRPS    := '';
  ConfigURL.ProConsultaNFSeRPS    := '';
  ConfigURL.ProConsultaSitLoteRPS := '';
  ConfigURL.ProConsultaNFSe       := '';
  ConfigURL.ProCancelaNFSe        := '';

  Result := ConfigURL;
end;

