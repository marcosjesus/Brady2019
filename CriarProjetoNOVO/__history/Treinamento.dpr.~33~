program Treinamento;

uses
  Forms,
  Vcl.Dialogs,
  Winapi.Windows,
  System.SysUtils,
  UPrincipal in 'UPrincipal.pas' {frmPrincipal},
  DBConect in 'DBConect.pas' {DB_Conect: TDataModule},
