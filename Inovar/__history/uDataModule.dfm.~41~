  object FDQueryNotaFiscal: TFDQuery
    Connection = FDConnectionFirebird
    SQL.Strings = (
      'SELECT SUM(A01.VNOTA) AS TOTAL_NF'
      
        'FROM ENTREGA A01 INNER JOIN NATOPER B01 ON ( A01.CODAUTONATOPER ' +
        '= B01.CODIGO )'
      'WHERE 1 = 1'
      '  AND A01.SAIDA >= :SAIDA_INI AND A01.SAIDA <= :SAIDA_FIM'
      '  AND A01.STATUS = '#39'EMIT'#39
      '  AND B01.EMITETIT <> '#39'T'#39
      '  AND A01.CGCCLIE = :CGCCLIE')
    Left = 472
    Top = 232
    ParamData = <
      item
        Name = 'SAIDA_INI'
        DataType = ftDate
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'SAIDA_FIM'
        DataType = ftDate
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'CGCCLIE'
        DataType = ftString
        ParamType = ptInput
        Value = Null
      end>
